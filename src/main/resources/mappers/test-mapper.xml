<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fishqq.mybatis.example.TestMapper">
    <select id="all" resultType="com.fishqq.mybatis.example.TestDo">
        SELECT id, name
        FROM test order by id desc
    </select>

    <select id="listByPage" resultType="com.fishqq.mybatis.example.TestDo">
        SELECT id, name FROM test order by id asc
    </select>

    <insert id="insert" parameterType="com.fishqq.mybatis.example.TestDo">
        insert into test(id, name) values(#{id}, #{name})
    </insert>
</mapper>
